{
	"name": "@squarecloud/api",
	"version": "3.6.5",
	"description": "A NodeJS wrapper for Square Cloud API",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"module": "lib/index.mjs",
	"scripts": {
		"release": "npm run lint && npm run build && changeset publish",
		"build": "tsup ./src",
		"check-types": "tsc --noEmit",
		"lint": "biome check --write .",
		"test": "ts-node test/index.ts",
		"prepare": "husky"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"dependencies": {
		"@squarecloud/api-types": "^0.3.2",
		"form-data": "^4.0.1",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@changesets/cli": "^2.27.9",
		"@types/node": "^22.8.6",
		"husky": "^9.1.6",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"tsc-alias": "^1.8.10",
		"tsup": "^8.3.5",
		"typescript": "^5.6.3"
	},
	"keywords": [
		"wrapper",
		"square",
		"squarecloud",
		"api",
		"typescript",
		"app",
		"bot",
		"website",
		"host"
	],
	"author": {
		"name": "joaotonaco",
		"url": "https://github.com/joaotonaco"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/squarecloudofc/sdk-api-js.git"
	},
	"bugs": {
		"url": "https://github.com/squarecloudofc/sdk-api-js/issues"
	},
	"homepage": "https://docs.squarecloud.app/sdks/js/client",
	"license": "MIT"
}
